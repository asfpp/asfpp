<?xml version="1.0"?>
<configuration>
	<Unconditional>
		<Attack>
			<start_time>200</start_time>
			<frequency>0.035</frequency>
			<var><name>seqno</name><value>10000.0</value><type>NUMBER</type></var>
			<var><name>15</name><value>15</value><type>NUMBER</type></var>
			<var><name>1</name><value>1</value><type>NUMBER</type></var>
			<var><name>0</name><value>0</value><type>NUMBER</type></var>
			<var><name>4</name><value>4</value><type>NUMBER</type></var>

			<action>
				<name>Create</name>
				<parameters>packetName:fakePacket:APP.type:0</parameters>
			</action>
			<action>
				<name>Change</name>
				<parameters>field_name:APP.data:value:15:packetName:fakePacket</parameters>
			</action>
			<action>
				<name>Change</name>
				<parameters>field_name:APP.digest:value:0:packetName:fakePacket</parameters>
			</action>
			<action>
				<name>Change</name>
				<parameters>field_name:APP.sourceNodeID:value:4:packetName:fakePacket</parameters>
			</action>
			<action>
				<name>Change</name>
				<parameters>field_name:APP.sequenceNumber:value:seqno:packetName:fakePacket</parameters>
			</action>
			<action>
				<name>Put</name>
				<parameters>direction:RX:nodes:1:throughWC:true:delay:1.5:packetName:fakePacket</parameters>
			</action>
			<action>
				<name>Expression</name>
				<item>1</item>
				<item>+=</item>
				<item>seqno</item>
			</action>

		</Attack>
	</Unconditional>
</configuration>
