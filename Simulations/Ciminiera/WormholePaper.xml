<?xml version="1.0"?>
<configuration>
	<Physical>
		<Attack>
			<node>5</node>
			<start_time>200</start_time>
			<action>
				<name>Move</name>
				<parameters>99:120:0</parameters>
			</action>
		</Attack>
	</Physical>

	<Conditional>
		<Attack>
			<node>5</node>
			<start_time>202</start_time>
			<filter>APP.sourceNodeID:==:5:AND</filter>
			<var><name>seqno</name><value>10000.0</value><type>NUMBER</type></var>
			<var><name>1</name><value>1</value><type>NUMBER</type></var>

			<action>
				<name>Clone</name>
				<parameters>packetName:original:newPacketName:wormPacket</parameters>
			</action>

			<action>
				<name>Change</name>
				<parameters>field_name:APP.sequenceNumber:value:seqno:packetName:wormPacket</parameters>
			</action>
		
			<action>
				<name>Put</name>
				<parameters>direction:RX:nodes:1:delay:1.5:throughWC:false:packetName:original</parameters>
			</action>
			<action>
				<name>Put</name>
				<parameters>direction:RX:nodes:1:delay:1.5:throughWC:false:packetName:wormPacket</parameters>
			</action>


			<action>
				<name>Drop</name>
				<parameters>packetName:original</parameters>
			</action>
			
			<action>
				<name>Expression</name>
				<item>1</item>
				<item>+=</item>
				<item>seqno</item>
			</action>

		</Attack>

	</Conditional>
</configuration>
