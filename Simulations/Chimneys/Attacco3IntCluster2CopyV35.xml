<?xml version="1.0"?>
<configuration>

	<Conditional>
		

		<Attack>
			<node>15</node>
			<start_time>180</start_time>
			<filter>NET.source:==:15:NET.id:==:6:AND</filter>
			<var><name>1</name><value>1</value><type>NUMBER</type></var>
			<var><name>35</name><value>35</value><type>NUMBER</type></var>
			<var><name>data</name><value>0</value><type>NUMBER</type></var>
			<var><name>seqNoApp</name><value>40000</value><type>NUMBER</type></var>

			<action>
				<name>Retrieve</name>
				<parameters>field_name:APP.data:varName:data:packetName:original</parameters>
			</action>

			<action>
				<name>Expression</name>
				<item>35</item>
				<item>-=</item>
				<item>data</item>
			</action>

			<action>
				<name>Change</name>
				<parameters>field_name:APP.data:value:data:packetName:original</parameters>
			</action>

			<action>
				<name>Clone</name>
				<parameters>packetName:original:newPacketName:copy</parameters>
			</action>

			<action>
				<name>Clone</name>
				<parameters>packetName:original:newPacketName:copy2</parameters>
			</action>

			<action>
				<name>Expression</name>
				<item>1</item>
				<item>+=</item>
				<item>seqNoApp</item>
			</action>


			<action>
				<name>Change</name>
				<parameters>packetName:original:field_name:APP.sequenceNumber:value:seqNoApp</parameters>
			</action>

			
			<action>
				<name>Expression</name>
				<item>1</item>
				<item>+=</item>
				<item>seqNoApp</item>
			</action>


			<action>
				<name>Change</name>
				<parameters>packetName:copy:field_name:APP.sequenceNumber:value:seqNoApp</parameters>
			</action>

			<action>
				<name>Send</name>
				<parameters>packetName:copy</parameters>
			</action>

			<action>
				<name>Expression</name>
				<item>1</item>
				<item>+=</item>
				<item>seqNoApp</item>
			</action>

			<action>
				<name>Change</name>
				<parameters>packetName:copy2:field_name:APP.sequenceNumber:value:seqNoApp</parameters>
			</action>

			<action>
				<name>Send</name>
				<parameters>packetName:copy2</parameters>
			</action>

		</Attack>	

	</Conditional>	

</configuration>
