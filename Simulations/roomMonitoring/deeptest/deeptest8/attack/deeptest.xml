<?xml version="1.0"?>

<configuration>

	<Conditional>

		<Attack>
			<start_time>50</start_time>
			<node>3:4:9:10:15:16</node>
			<var><name>110</name><value>110</value><type>NUMBER</type></var>
			<filter>[:APP.sourceNodeID:==:3|4|9|10|15|16:]</filter>

			<action>
				<name>Clone</name>
				<parameters>packetName:original:newPacketName:fakePacket</parameters>
			</action>

			<action>
				<name>Drop</name>
				<parameters>packetName:original:threshold:1</parameters>
			</action>

			<action>
				<name>Change</name>
				<parameters>packetName:fakePacket:field_name:APP.data:value:110</parameters>
			</action>

			<action>
				<name>Send</name>
				<parameters>packetName:fakePacket:delay:0</parameters>
			</action>
		</Attack>

	</Conditional>

</configuration>